{
   "action": {
      "default_icon": "icon-256.png",
      "default_popup": "popup.html",
      "default_title": "Glow - Solana Wallet"
   },
   "background": {
      "service_worker": "realms/background.js"
   },
   "content_scripts": [ {
      "js": [ "realms/content.js" ],
      "matches": [ "https://*/*", "*://localhost/*" ],
      "run_at": "document_start"
   } ],
   "description": "A fast, easy to use Solana wallet",
   "icons": {
      "256": "icon-256.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Glow - Solana Wallet BETA",
   "permissions": [ "storage", "tabs", "alarms", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.60.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "realms/inpage.js" ]
   } ]
}
